<div>
   <table class="index_table" >
   <thead>
      <tr>
         <th>id</th>
         <th>Name</th>
         <th>Actions</th>
      </tr>
   </thead>
   <tbody>
      <% @users.each do |user|%>
      <tr>
         <td><%= user.id %></td>
         <td><%= user.name %></td>
    <div><td class='Action'>
               <%=link_to 'Show',user_path(user) %>
               <%=link_to 'Edit',edit_user_path(user) %>
               <%=link_to "Delete",user_path(user),method: :delete,data: {confirm: "Are you sure you want to delete this user?"}%></td></div>
      </tr>
      <%end%>
   </tbody>
   </table><br>
